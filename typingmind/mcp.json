{
  "mcpServers": {
    "typingmind-docs-mcp": {
      "command": "uvx",
      "args": [
        "--from",
        "mcpdoc",
        "mcpdoc",
        "--urls",
        "TypingMind:https://docs.typingmind.com/llms.txt",
        "--allowed-domains",
        "docs.typingmind.com",
        "--transport",
        "stdio"
      ]
    }
  }
}
